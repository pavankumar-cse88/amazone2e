<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme -->
        <!-- standard, dark -->
        <theme>dark</theme>

        <!-- document encoding -->
        <!-- defaults to UTF-8 -->
        <encoding>UTF-8</encoding>

        <!-- protocol for script and stylesheets -->
        <!-- defaults to https -->
        <protocol>https</protocol>

        <!-- title of the document -->
        <documentTitle>Execution report</documentTitle>

        <!-- report name - displayed at top-nav -->
        <reportName>Build</reportName>

        <!-- timestamp format -->
        <timeStampFormat>MMM dd, yyyy HH:mm:ss</timeStampFormat>

        <!-- custom javascript -->
        <scripts>
            <![CDATA[
                $(document).ready(function() {

                });
            ]]>
        </scripts>

        <!-- custom styles -->
        <styles>
            <![CDATA[
                .r-img { width: 25%; }

                ul.screenshots {
                    overflow-x: scroll;
                    white-space: nowrap;
                }

                ::-webkit-scrollbar {
                    height: 0.5em
                }
            ]]>
        </styles>
    </configuration>
</extentreports>
